<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity_six_week_courses">

    <TextView
        android:id="@+id/sixWeekCoursesLabel"
        android:layout_width="192dp"
        android:layout_height="52dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="16dp"
        android:text="Six-Week Courses"
        android:textSize="21sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText,MissingConstraints" />

    <Button
        android:id="@+id/backButton1"
        android:layout_width="99dp"
        android:layout_height="50dp"
        android:layout_marginStart="81dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="#657985"
        android:text="Back"
        android:textColorLink="#009688"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/sixWeekCoursesLabel"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText,MissingConstraints" />

    <ListView
        android:id="@+id/sixWeekCoursesList"
        android:layout_width="394dp"
        android:layout_height="720dp"
        android:layout_marginStart="7dp"
        android:layout_marginTop="18dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sixWeekCoursesLabel"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>